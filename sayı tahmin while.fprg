    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="say&#305; tahmin oyunu"/>
            <declare name="rastgele, sayac, tahmin" type="Integer" array="False" size=""/>
            <assign variable="sayac" expression="1"/>
            <assign variable="tahmin" expression="0"/>
            <assign variable="rastgele" expression="random(100)"/>
            <output expression="&quot;rastgele&quot;" newline="True"/>
            <output expression="&quot;tahmin giriniz&quot;" newline="True"/>
            <input variable="tahmin"/>
            <while expression="tahmin!=rastgele">
