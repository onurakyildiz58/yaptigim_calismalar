                    <then>
                        <output expression="&quot;daha k&#252;&#231;&#252;k de&#287;er gir&quot;" newline="True"/>
                    </then>
                    <else>
                        <output expression="&quot;daha b&#252;y&#252;k de&#287;er gir&quot;" newline="True"/>
                    </else>
                </if>
                <input variable="tahmin"/>
                <assign variable="sayac" expression="sayac + 1"/>
            </while>
            <output expression="&quot;tebrikler bildiniz&quot;" newline="True"/>
        </body>
